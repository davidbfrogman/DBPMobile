
@{
    ViewBag.Title = "DenverFashionPhotographer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model List<DBPMobile.Models.PortfolioImage>

    @foreach (var image in Model)
    {
        <img src="@image.ImageLocation" align="middle" style="border-width:0px;margin-bottom:5px; max-width:100%; height:auto; margin-right:auto; margin-left:auto; display:block;" /><br/>
    }